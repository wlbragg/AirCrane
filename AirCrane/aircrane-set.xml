<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList include="Systems/aircrane-base.xml">

  <sim>

    <description>AirCrane (Yasim)</description>
    <author>BARANGER Emmanuel (3D), Heiko Schulz (FDM), Wayne Bragg (Utility 3D and Systems, Effects)</author>
    <status>alpha</status>
    <aircraft-version>2019.2-git</aircraft-version>
    <minimum-fg-version>2018.3.4</minimum-fg-version>
    <license>GPL v2</license>
    <flight-model>yasim</flight-model>
    <aero>Systems/aircrane-yasim</aero>

    <tags>
      <tag>Sikorsky</tag>
      <tag>helicopter</tag>
      <tag>2-engine</tag>
      <tag>1960s</tag>
      <tag>1970s</tag>
      <tag>1980s</tag>
      <tag>1990s</tag>
      <tag>2000s</tag>
    </tags>

    <rating>
      <FDM type="int">     4 </FDM>
      <systems type="int"> 1 </systems>
      <cockpit type="int"> 1 </cockpit>
      <model type="int">   4 </model>
    </rating>
	        <!-- Splash screens. One is randomly chosen when FlightGear starts -->
        <previews>
            <preview>
                <type>cockpit</type>
                <path>Previews/aircrane-preview1.jpg</path>
                <splash type="bool">true</splash>
            </preview>
            <preview>
                <type>exterior</type>
                <path>Previews/aircrane-preview2.jpg</path>
                <splash type="bool">true</splash>
            </preview>
            <preview>
                <type>exterior</type>
                <path>Previews/aircrane-preview3.jpg</path>
                <splash type="bool">true</splash>
            </preview>
        </previews>

  </sim>

</PropertyList>
