<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <title>AirCrane - background and help</title>
  <line>______________________________________________</line>
  <key>
    <name>{</name>
    <desc>Start engines</desc>
  </key>
  <key>
    <name>}</name>
    <desc>Shutdown engines</desc>
  </key>
  <key>
    <name>R</name>
    <desc>Toggle rotor brake</desc>
  </key>
  <key>
    <name>r</name>
    <desc>Apply rotor brake</desc>
  </key>
  <key>
    <name>Ctrl d</name>
    <desc>Toggle S.A.S.</desc>
  </key>
  <key>
    <name>Ctrl e</name>
    <desc>Toggle C.A.S.</desc>
  </key>
  <key>
    <name>Ctrl f</name>
    <desc>Auto hover</desc>
  </key>
  <key>
    <name>j</name>
    <desc>Toggle Ram Scoop Deploy</desc>
  </key>
  <key>
    <name>J</name>
    <desc>Toggle Water Sniffer Deploy</desc>
  </key>
  <key>
    <name>k</name>
    <desc>Toggle Cannon Valve</desc>
  </key>
  <key>
    <name>K</name>
    <desc>Toggle Tank Doors</desc>
  </key>
  <key>
    <name>O</name>
    <desc>Attach cargo/auto attach off</desc>
  </key>
  <key>
    <name>o</name>
    <desc>Release cargo</desc>
  </key>
    <key>
    <name>Ctrl o</name>
    <desc>Toggle Auto attach cargo</desc>
  </key>
  <key>
    <name>w</name>
    <desc>Winch retract</desc>
  </key>
  <key>
    <name>W</name>
    <desc>Winch extend</desc>
  </key>
  
</PropertyList>
