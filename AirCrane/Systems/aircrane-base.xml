            <int   n="5"  type="int">0</int>
            
            <int   n="6" alias="/sim/cargo/cargo1-onhook"/>
            <int   n="7" alias="/sim/cargo/cargo2-onhook"/>
            <int   n="8" alias="/sim/cargo/cargo3-onhook"/>
            <int   n="9" alias="/sim/gui/dialogs/aicargo-dialog/connection"/>
            <int   n="10" alias="/sim/cargo/rope/pulling"/>

            <float n="0" alias="/sim/model/door-positions/crew/position-norm"/>
            <float n="1" alias="/sim/model/door-positions/passenger/position-norm"/>
            <!--float n="2" alias="/position/gear-agl-m"/-->
            <float n="2" alias="/position/true-agl-ft"/>
            <float n="3" alias="/position/altitude-ft">0</float>
            <float n="4" alias="/engines/engine[0]/rpm"/>
            <float n="5" alias="/sim/model/firetank/deployramscoop/position-norm"/>
            <float n="6" alias="/sim/model/firetank/deployflexhose/position-norm"/>
            <float n="7" alias="/sim/model/door-positions/engineer/position-norm"/>
            <float n="8" alias="/sim/model/door-positions/starboard/position-norm"/>
            <float n="9" alias="/sim/model/hitch/hitchhook/position-norm"/>
            <float n="10" alias="/sim/model/hitch/hitchstop/position-norm"/>
            <float n="11" alias="/sim/model/watercannon/position-deg"/>
            <float n="12" alias="/controls/electric/wipers/position-norm"/>
