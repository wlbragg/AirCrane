            <bool n="8"  alias="/sim/model/lights/sbc1/state"/>
            <bool n="9"  alias="/sim/model/lights/beacon/enabled"/>
            <bool n="10" alias="/sim/model/aircrane/lighting/beaconfwd-light"/>
            <bool n="11" alias="/sim/model/aircrane/lighting/beaconaft-light"/>
            <bool n="12" alias="/sim/model/aircrane/lighting/strobe-light"/>
            <bool n="13" alias="/sim/model/aircrane/lighting/nav-lights"/>
            <bool n="14" alias="/sim/model/aircrane/lighting/spot-light"/>
            <bool n="15" alias="/sim/model/aircrane/lighting/landing-light"/>
            
          </generic>
        </multiplay>

        <weight n="0">
          <name>Pilot</name>
          <weight-lb>180</weight-lb>
          <max-lb>300</max-lb>
        </weight>

        <weight n="1">
          <name>Co-Pilot</name>
          <weight-lb>180</weight-lb>
          <max-lb>300</max-lb>
        </weight>

        <weight n="2">
          <name>Right Passenger</name>
          <weight-lb>0</weight-lb>
          <max-lb>300</max-lb>
        </weight>

        <weight n="3">
          <name>Load</name>
          <weight-lb>0</weight-lb>
          <max-lb>20000</max-lb>
        </weight>

        <aircraft-data include="aircraft-data.xml"/>

        <help include="help.xml"/>

        <menubar include="../Dialogs/aircrane-menu.xml"/>

        <flight-recorder include="flight-recorder.xml"/>

		<checklists include="Aircraft/AirCrane/Systems/checklists.xml"/>
        <tutorials include="Aircraft/AirCrane/Tutorials/AirCraneTutorials.xml"/>

        <instrumentation>
            <path>Aircraft/AirCrane/Systems/instrumentation.xml</path>
        </instrumentation>

    </sim>

    <input include="aircrane-keyboard.xml"/>

    <environment include="environment.xml"/>

    <controls include="controls.xml"/>

    <rotors include="rotors.xml"/>

    <consumables include="consumables.xml"/>

    <instrumentation include="instrumentation-init.xml"/>

    <engines>
        <engine-rpm type="float">0</engine-rpm>
