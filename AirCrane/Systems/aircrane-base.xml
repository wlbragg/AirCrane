            <int type="int" n="10" alias="controls/armament/trigger">0</int>

            <string n="9" type="string" />
            
            <bool n="0"  alias="/sim/model/extendedcab/enabled"/>
            <bool n="1"  alias="/sim/model/tandemwheels/enabled"/>
            <bool n="2"  alias="/sim/model/firetank/enabled"/>
