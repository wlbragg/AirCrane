              <roll27  type="double">0.0</roll27>
              <roll28  type="double">0.0</roll28>
              <roll29  type="double">0.0</roll29>
              <roll30  type="double">0.0</roll30>
              <roll31  type="double">0.0</roll31>
            </flexhose>
          </firetank>
          <extendedcab>
              <enabled type="bool">false</enabled>
          </extendedcab>
          <tandemwheels>
              <enabled type="bool">false</enabled>
          </tandemwheels>
          <garmin196-visible type="bool">false</garmin196-visible>
        </model>

        <submodels>
          <serviceable type="bool">true</serviceable>
          <path>Aircraft/AirCrane/Models/submodels.xml</path>
        </submodels>

        <systems>
            <path>Aircraft/AirCrane/Systems/systems.xml</path>

            <autopilot n="0">
                <path>Aircraft/AirCrane/Systems/engineering-station.xml</path>
            </autopilot>

            <autopilot n="1">
                <path>Aircraft/AirCrane/Systems/fcs.xml</path>
            </autopilot>

            <autopilot n="2">
              <path>Aircraft/AirCrane/Systems/afcs.xml</path>
            </autopilot>

            <property-rule n="100">
              <path>Aircraft/AirCrane/Systems/lighting.xml</path>
            </property-rule>

            <property-rule n="101">
              <path>Aircraft/AirCrane/Systems/model-effects.xml</path>
            </property-rule>

            <property-rule n="102">
              <path>Aircraft/AirCrane/Systems/electrical.xml</path>
            </property-rule>

            <property-rule n="104">
              <path>Aircraft/AirCrane/Systems/fuel.xml</path>
            </property-rule>
        </systems>

        <multiplay>
          <chat_display>1</chat_display>
          <generic>
            <int   n="0"  type="int">0</int>
            <int   n="1"  type="int">0</int>
            <int   n="2"  type="int">0</int>
            <int   n="3"  type="int">0</int>
