                </not>
                <not>
                  <property>/controls/switches/fuelheatereng1</property>
                </not>
                <property>/systems/fuel/temp-start-calc</property>
              </and>
          </condition>
      </enable>
      <input>
        <property>/systems/fuel/temp-fwd-heat</property>
      </input>
      <output>
        <property>/systems/fuel/temp-fwd-heat</property>
      </output>
  </filter>

