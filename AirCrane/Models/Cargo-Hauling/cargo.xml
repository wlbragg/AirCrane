<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <model>
    <path>shipping-container-big.ac</path>
    <name>shipping-container-big</name>
    <condition>
      <and>
        <or>
          <property>sim/model/cargo1-onhook</property>
          <property>sim/model/cargo2-onhook</property>
          <property>sim/model/cargo3-onhook</property>
        </or>
        <not>
          <property>/sim/gui/dialogs/aicargo-dialog/rope</property>
        </not>
      </and>
    </condition>
    <offsets>
      <x-m>-2.5</x-m>
      <y-m>0</y-m>
      <z-m>-3.85</z-m>
      <pitch-deg>0</pitch-deg>
    </offsets>
  </model>

  <model>
    <path>cub.ac</path>
    <name>cub</name>
    <condition>
      <and>
        <property>sim/model/cargo4-onhook</property>
        <not>
          <property>/sim/gui/dialogs/aicargo-dialog/rope</property>
        </not>
      </and>
    </condition>
    <offsets>
      <x-m>-2.5</x-m>
      <y-m>0</y-m>
      <z-m>-3.85</z-m>
      <pitch-deg>0</pitch-deg>
    </offsets>
  </model>

  <model>
    <path>groundtank.ac</path>
    <name>ground-tank</name>
    <condition>
      <and>
        <property>sim/model/cargo5-onhook</property>
        <not>
          <property>/sim/gui/dialogs/aicargo-dialog/rope</property>
        </not>
      </and>
    </condition>
    <offsets>
      <x-m>-2.5</x-m>
      <y-m>0</y-m>
      <z-m>-3.85</z-m>
      <pitch-deg>0</pitch-deg>
    </offsets>
  </model>

  <model>
    <path>tankstand.ac</path>
    <name>tank-stand</name>
    <condition>
      <and>
        <property>sim/model/cargo6-onhook</property>
        <not>
          <property>/sim/gui/dialogs/aicargo-dialog/rope</property>
        </not>
      </and>
    </condition>
    <offsets>
      <x-m>-2.5</x-m>
      <y-m>0</y-m>
      <z-m>-3.85</z-m>
      <pitch-deg>0</pitch-deg>
    </offsets>
  </model>

</PropertyList>