    <!-- Glass model F -->
    <object-name>intvitreF</object-name>
    <object-name>intvitresreardoorF</object-name>
    <object-name>intvitresstarboardF</object-name>
  </animation>

  <effect>
    <inherits-from>Aircraft/AirCrane/Models/Effects/Interior/cockpit-interior</inherits-from>
    <!-- Objets opaques -->
    <object-name>interieur</object-name>
    <object-name>tour</object-name>
    <object-name>planchet</object-name>
    <object-name>floor</object-name>
    <object-name>reposepieds</object-name>
    <object-name>accoudoir</object-name>
    <object-name>gruesiege1</object-name>
    <object-name>gruesiege2</object-name>
    <object-name>plaque</object-name>
    <object-name>intporteG</object-name>
    <object-name>intporteD</object-name>
    <object-name>harness</object-name>

    <!-- Model E -->
    <object-name>interieurE</object-name>
    <object-name>tourE</object-name>
    <object-name>planchetE</object-name>
    <object-name>grueinstrE</object-name>
    <object-name>intreardoorE</object-name>

    <!-- Model F -->
    <object-name>interieurF</object-name>
    <object-name>tourF</object-name>
    <object-name>floorF</object-name>
    <object-name>planchetF</object-name>
    <object-name>tourF</object-name>
    <object-name>barreF</object-name>
    <object-name>grueinstrF</object-name>
    <object-name>intreardoorF</object-name>
    <object-name>intstarboarddoorF</object-name>
  </effect>

  <!-- working for Compositor -->
  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>intvitres</object-name>
    <object-name>intvitresporteG</object-name>
    <object-name>intvitresporteD</object-name>
    <!-- Glass model E -->
    <object-name>intvitreE</object-name>
    <object-name>intvitresreardoorE</object-name>
    <!-- Glass model F -->
    <object-name>intvitreF</object-name>
    <object-name>intvitresreardoorF</object-name>
    <object-name>intvitresstarboardF</object-name>
  </effect>

  <animation>
    <type>select</type>
    <condition>
      <property>sim/model/extendedcab/enabled</property>
    </condition>
    <object-name>interieurF</object-name>
    <object-name>tourF</object-name>
    <object-name>floorF</object-name>
    <object-name>planchetF</object-name>
    <object-name>tourF</object-name>
    <object-name>barreF</object-name>
    <object-name>grueinstrF</object-name>
    <object-name>intreardoorF</object-name>
    <object-name>intstarboarddoorF</object-name>
    <object-name>intvitreF</object-name>
    <object-name>intvitresreardoorF</object-name>
    <object-name>intvitresstarboardF</object-name>
  </animation>

  <animation>
    <type>select</type>
    <condition>
      <not><property>sim/model/extendedcab/enabled</property></not>
    </condition>
    <object-name>interieurE</object-name>
    <object-name>tourE</object-name>
    <object-name>planchetE</object-name>
    <object-name>grueinstrE</object-name>
    <object-name>intreardoorE</object-name>
    <object-name>intvitreE</object-name>
    <object-name>intvitresreardoorE</object-name>
  </animation>

  <!-- Panel -->
  <model>
    <path>Aircraft/AirCrane/Models/Interior/Panel/panel.xml</path>
    <offsets>
      <x-m> -9.39232 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m> -1.50912 </z-m>
    </offsets>
  </model>

