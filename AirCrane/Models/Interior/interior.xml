  </animation>

  <animation>
    <type>rotate</type>
    <object-name>IntLaPorteDroite</object-name>
    <property>sim/model/door-positions/passenger/position-norm</property>
    <factor> 100 </factor>
    <axis>
      <x1-m> -9.06146 </x1-m>
      <y1-m>  1.08731 </y1-m>
      <z1-m> -1.57367 </z1-m>
      <x2-m> -8.87113 </x2-m>
      <y2-m>  0.92431 </y2-m>
      <z2-m> -0.79338 </z2-m>
    </axis>
  </animation>

