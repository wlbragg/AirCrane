      <x-m> -2.377910 </x-m>
      <y-m>  0.016045 </y-m>
      <z-m> -1.100290 </z-m>
    </offsets>
  </model>

  <!-- Pilote -->
  <model>
    <path>Aircraft/AirCrane/Models/Pilot/pilot.xml</path>
    <condition>
      <property>sim/model/occupants</property>
    </condition>
    <offsets>
      <x-m> -8.892 </x-m>
      <y-m>  0.66314 </y-m>
      <z-m> -1.625 </z-m>
    </offsets>
  </model>

  <!-- animation des portes -->
  <animation>
    <name>IntLaPorteGauche</name>
    <object-name>intporteG</object-name>
    <object-name>intvitresporteG</object-name>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>IntLaPorteGauche</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>doors.crew.toggle();</script>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>IntLaPorteGauche</object-name>
    <property>sim/model/door-positions/crew/position-norm</property>
    <factor> -100 </factor>
    <axis>
      <x1-m> -9.06146 </x1-m>
      <y1-m> -1.08691 </y1-m>
      <z1-m> -1.57367 </z1-m>
      <x2-m> -8.87113 </x2-m>
      <y2-m> -0.92390 </y2-m>
      <z2-m> -0.79338 </z2-m>
    </axis>
  </animation>

  <animation>
    <name>IntLaPorteDroite</name>
    <object-name>intporteD</object-name>
    <object-name>intvitresporteD</object-name>
  </animation>

  <animation>
    <type>pick</type>
