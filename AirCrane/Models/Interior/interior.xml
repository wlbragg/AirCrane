    <object-name>gruesiege2</object-name>
    <object-name>plaque</object-name>
    <object-name>intporteG</object-name>
    <object-name>intporteD</object-name>
    <object-name>harness</object-name>

    <!-- Model E -->
    <object-name>interieurE</object-name>
    <object-name>tourE</object-name>
    <object-name>planchetE</object-name>
    <object-name>grueinstrE</object-name>
    <object-name>intreardoorE</object-name>

    <!-- Model F -->
    <object-name>interieurF</object-name>
    <object-name>tourF</object-name>
    <object-name>floorF</object-name>
    <object-name>planchetF</object-name>
    <object-name>tourF</object-name>
    <object-name>barreF</object-name>
    <object-name>grueinstrF</object-name>
    <object-name>intreardoorF</object-name>
