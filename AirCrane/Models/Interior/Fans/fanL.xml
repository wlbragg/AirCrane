	<property>controls/switches/switchpilotfan</property>
	<factor>100</factor>
	<axis>
		<x>1</x>
		<y>0</y>
		<z>0</z>
	</axis>
	<center>
		<x-m> 0.037505</x-m>
		<y-m>-0.000382</y-m>
		<z-m> 0.13158</z-m>
	</center>
  </animation>

</PropertyList>
