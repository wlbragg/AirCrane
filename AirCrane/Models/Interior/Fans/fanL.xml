<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>fanL.ac</path>

  <effect>
    <inherits-from>Aircraft/AirCrane/Models/Effects/Interior/lm-interior-fanl-white-plastic</inherits-from>
	<object-name>fan</object-name>
  </effect>

  <effect>
    <inherits-from>Aircraft/AirCrane/Models/Effects/Interior/lm-interior-fanl-wiring-harness</inherits-from>
	<object-name>fanWiring</object-name>
  </effect>

  <effect>
    <inherits-from>Aircraft/AirCrane/Models/Effects/Interior/lm-interior-fanl-worn-metal</inherits-from>
    <object-name>fanBase</object-name>
  </effect>	

  <effect>
    <inherits-from>Aircraft/AirCrane/Models/Effects/Interior/lm-interior-fanl-worn-shiny-metal</inherits-from>
	<object-name>fanBolts</object-name>
	<object-name>fanRod</object-name>
