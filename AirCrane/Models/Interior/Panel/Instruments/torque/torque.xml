<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>torque.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>Lneedle</object-name>
    <object-name>Rneedle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>vitre</object-name>
  </effect>
  
  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>Lneedle</object-name>
    <object-name>Rneedle</object-name>
    <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.04 </red>
      <green> 0.05 </green>
      <blue>  0.04 </blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Lneedle</object-name>
    <property>sim/model/aircrane/torque-pct</property>
    <interpolation>
      <entry><ind>   0 </ind><dep> -205 </dep></entry>
      <entry><ind> 220 </ind><dep>  -90 </dep></entry>
      <entry><ind> 440 </ind><dep>   25 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Rneedle</object-name>
    <property>sim/model/aircrane/torque-pct</property>
    <interpolation>
      <entry><ind>   0 </ind><dep> -205 </dep></entry>
      <entry><ind> 220 </ind><dep>  -90 </dep></entry>
      <entry><ind> 440 </ind><dep>   25 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
