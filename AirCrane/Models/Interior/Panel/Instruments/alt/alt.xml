        <object-name>inhg-c</object-name>
        <property>/instrumentation/altimeter/setting-inhg</property>
        <interpolation>
            <entry>
                <ind>27.90</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>28.0</ind>
                <dep>9.35</dep>
            </entry>
            <entry>
                <ind>28.5</ind>
                <dep>59.0</dep>
            </entry>
            <entry>
                <ind>29.0</ind>
                <dep>109.0</dep>
            </entry>
            <entry>
                <ind>29.5</ind>
                <dep>159.5</dep>
            </entry>
            <entry>
                <ind>29.9</ind>
