<?xml version="1.0"?>

<PropertyList>

<path>fuelpress.ac</path>

  <effect>
      <inherits-from>Models/Effects/interior/cockpit-interior-shadows</inherits-from>
      <object-name>case</object-name>
      <object-name>face</object-name>
      <object-name>fasteners</object-name>

      <object-name>needle-aux</object-name>
      <object-name>needle-fwd</object-name>
      <object-name>needle-aft</object-name>
      <object-name>bug-fwd</object-name>
      <object-name>bug-aft</object-name>
      <object-name>knob-fwd</object-name>
      <object-name>knob-aft</object-name>
  </effect>
  <!--effect>
      <inherits-from>Models/Effects/interior/cockpit-flashlight</inherits-from>
      <object-name>needle-aux</object-name>
      <object-name>needle-fwd</object-name>
      <object-name>needle-aft</object-name>
      <object-name>bug-fwd</object-name>
