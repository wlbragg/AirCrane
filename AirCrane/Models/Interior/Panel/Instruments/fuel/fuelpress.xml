  <effect>
      <inherits-from>Models/Effects/interior/interior-glass-reflection-panel-front</inherits-from>
      <object-name>glass</object-name>
  </effect>

	<animation>
		<type>rotate</type>
    <object-name>needle-l</object-name>
    <property>consumables/fuel/fwd/pressure</property>
		<interpolation>
			<entry><ind>-99</ind><dep>     1</dep></entry>
			<entry><ind>  0</ind><dep>     0</dep></entry>
      <entry><ind>  1.5</ind><dep> -81</dep></entry>
      <entry><ind>  2.0</ind><dep>-174</dep></entry>
			<entry><ind>  2.5</ind><dep>-271</dep></entry>
			<entry><ind> 99</ind><dep>  -272</dep></entry>
		</interpolation>
		<axis>
			  <x1-m>-9.38687</x1-m>
        <y1-m>-0.003852</y1-m> 
        <z1-m>-1.52678</z1-m>
        <x2-m>-9.38598</x2-m>
        <y2-m>-0.003852</y2-m>
        <z2-m>-1.52668</z2-m>
    </axis>
	</animation>

	<animation>
		<type>rotate</type>
    <object-name>needle-h-l</object-name>
    <property>consumables/fuel/fwd/pressure</property>
		<interpolation>
			<entry><ind>-99</ind><dep>   1</dep></entry>
			<entry><ind>  0</ind><dep>   0</dep></entry>
      <entry><ind>  1</ind><dep> -38.5</dep></entry>
      <entry><ind>  2</ind><dep> -75</dep></entry>
			<entry><ind>  3</ind><dep>-111</dep></entry>
			<entry><ind>  4</ind><dep>-147</dep></entry>
			<entry><ind>  5</ind><dep>-181</dep></entry>
			<entry><ind>  6</ind><dep>-216</dep></entry>
      <entry><ind>  7</ind><dep>-251</dep></entry>
			<entry><ind>  8</ind><dep>-285</dep></entry>
			<entry><ind>  9</ind><dep>-322</dep></entry>
			<entry><ind> 10</ind><dep>-357</dep></entry>
			<entry><ind> 99</ind><dep>-358</dep></entry>
		</interpolation>
		<axis>
			  <x1-m>-9.38707</x1-m>
        <y1-m> 0.118176</y1-m> 
        <z1-m>-1.52539</z1-m>
        <x2-m>-9.38701</x2-m>
        <y2-m> 0.118172</y2-m>
        <z2-m>-1.52531</z2-m>
    </axis>
	</animation>

	<animation>
		<type>rotate</type>
    <object-name>bug-l</object-name>
    <property>consumables/fuel/fwd/bug</property>
		<interpolation>
			<entry><ind>-99</ind><dep>     1</dep></entry>
			<entry><ind>  0</ind><dep>     0</dep></entry>
      <entry><ind>  1.5</ind><dep> -81</dep></entry>
      <entry><ind>  2.0</ind><dep>-174</dep></entry>
			<entry><ind>  2.5</ind><dep>-271</dep></entry>
			<entry><ind> 99</ind><dep>  -272</dep></entry>
		</interpolation>
		<axis>
			  <x1-m>-9.38687</x1-m>
        <y1-m>-0.003852</y1-m> 
        <z1-m>-1.52678</z1-m>
        <x2-m>-9.38598</x2-m>
        <y2-m>-0.003852</y2-m>
        <z2-m>-1.52668</z2-m>
    </axis>
	</animation>

  <animation>
      <type>knob</type>
      <object-name>knob-l</object-name>
      <property>/consumables/fuel/fwd/bug</property>
      <factor>.01</factor>
      <axis>
			  <x1-m>-9.38407</x1-m>
        <y1-m> 0.024055</y1-m> 
        <z1-m>-1.55381</z1-m>
        <x2-m>-9.36764</x2-m>
        <y2-m> 0.023936</y2-m>
        <z2-m>-1.55198</z2-m>
    </axis>
      <action>
          <binding>
              <command>property-adjust</command>
              <property>/consumables/fuel/fwd/bug</property>
              <factor>0.01</factor>
              <min>0</min>
              <max>2.5</max>
              <wrap>false</wrap>
          </binding>
      </action>
      <hovered>
          <binding>
              <command>set-tooltip</command>
              <tooltip-id>alt-setting</tooltip-id>
              <label>Pressure Ratio: %1.1f</label>
              <property>/consumables/fuel/fwd/bug</property>
          </binding>
      </hovered>
  </animation>

