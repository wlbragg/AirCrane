    <animation>
      <type>select</type>
      <object-name>redoffl</object-name>
      <condition>
        <equals>
          <property>systems/electrical/outputs/eng1-underspeed</property>
          <value>0</value>
        </equals>
      </condition>
    </animation>
    <animation>
      <type>select</type>
      <object-name>redonl</object-name>
      <condition>
        <equals>
          <property>systems/electrical/outputs/eng1-underspeed</property>
          <value>1</value>
        </equals>
      </condition>
    </animation>
    <animation>
      <type>select</type>
      <object-name>redoffr</object-name>
      <condition>
        <equals>
          <property>systems/electrical/outputs/eng2-underspeed</property>
          <value>0</value>
        </equals>
      </condition>
    </animation>
    <animation>
      <type>select</type>
      <object-name>redonr</object-name>
      <condition>
        <equals>
          <property>systems/electrical/outputs/eng2-underspeed</property>
          <value>1</value>
        </equals>
      </condition>
    </animation>
    <animation>
        <type>material</type>
        <object-name>redonl</object-name>
        <emission>
            <red>  0.7</red>
            <green>0.5</green>
            <blue> 0.5</blue>
            <factor-prop>systems/electrical/outputs/eng1-underspeed</factor-prop>
        </emission>
    </animation>
    <animation>
        <type>material</type>
        <object-name>redonr</object-name>
