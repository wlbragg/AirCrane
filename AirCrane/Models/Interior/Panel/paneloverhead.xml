            <green>0.5</green>
            <blue> 0.5</blue>
            <factor-prop>systems/electrical/outputs/eng2-underspeed</factor-prop>
        </emission>
    </animation>

    <animation>
      <type>select</type>
      <object-name>lampferry</object-name>
      <condition>
        <greater-than>
          <property>sim/model/watercannon/position-deg</property>
          <value>-7.5</value>
        </greater-than>
      </condition>
    </animation>
    <animation>
      <type>select</type>
      <object-name>lampferryon</object-name>
      <condition>
        <equals>
          <property>sim/model/watercannon/position-deg</property>
          <value>-7.5</value>
        </equals>
      </condition>
    </animation>

    <animation>
      <type>select</type>
      <object-name>lamplanding</object-name>
      <condition>
        <less-than>
          <property>sim/model/watercannon/position-deg</property>
          <value>7.5</value>
        </less-than>
      </condition>
    </animation>
    <animation>
      <type>select</type>
      <object-name>lamplandingon</object-name>
      <condition>
        <equals>
          <property>sim/model/watercannon/position-deg</property>
          <value>7.5</value>
        </equals>
      </condition>
    </animation>

    <!-- Direct Drive "spring switch" -->
    <animation>
      <type>knob</type>
      <object-name>togglecannonpitch</object-name>
      <property>sim/model/watercannon/togglecannonpitch</property>
      <factor>-35</factor>
      <offset-deg>-10</offset-deg>
      <center>
        <x-m>-8.50175</x-m>
        <y-m>-0.154198</y-m>
        <z-m>-0.580201</z-m>
      </center>
      <axis>
        <x>0</x>
        <y>1</y>
        <z>0</z>
      </axis>
      <action>
        <binding>
          <command>property-adjust</command>
          <property>sim/model/watercannon/togglecannonpitch</property>
          <factor>1</factor>
          <min>-1</min>
          <max>1</max>
          <wrap>false</wrap>
        </binding>
        <binding>
          <command>nasal</command>
          <script>aircrane.click("avionics")</script>
        </binding>
      </action>
      <release>
        <binding>
          <command>property-assign</command>
          <property>sim/model/watercannon/togglecannonpitch</property>
          <value>0</value>
        </binding>
      </release>
      <hovered>
        <binding>
          <command>set-tooltip</command>
          <tooltip-id>togglecannonpitch</tooltip-id>
          <label>Cannon Pitch: %s</label>
          <property>sim/model/watercannon/togglecannonpitch</property>
          <mapping>nasal</mapping>
          <script>
            var modes = ['Neutral', 'Up', 'Down'];
            return modes[arg[0]];
          </script>
        </binding>
      </hovered>
    </animation>

    <animation>
      <type>knob</type>
      <object-name>fuelshutoffl</object-name>
