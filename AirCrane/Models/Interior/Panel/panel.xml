    <animation>
        <type>slider</type>
        <object-name>cplamptest</object-name>
        <property>controls/switches/cws-test</property>
        <repeatable>false</repeatable>
        <factor>0.002</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <action>
            <repeatable>false</repeatable>
            <binding>
                <command>nasal</command>
                <script>
                    setprop("/controls/switches/cws-test", 1);
                </script>
            </binding>
        </action>
        <release>
            <binding>
                <command>nasal</command>
                <script>
                    setprop("/controls/switches/cws-test", 0);
                    aircrane.click("avionics");
                </script>
            </binding>
        </release>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>cplamptest</tooltip-id>
                <label>CWS Test: %s</label>
                <mapping>on-off</mapping>
                <property>controls/switches/cws-test</property>
            </binding>
        </hovered>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>cpswitch</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>controls/switches/cws-bright</property>
            </binding>
        </action>
        <release>
          <binding>
            <command>property-assign</command>
            <property>controls/switches/cws-bright</property>
            <value>0</value>
          </binding>
        </release>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>cpswitch</tooltip-id>
                <label>CWS Brightness: %s</label>
                <mapping>Dim-Bright</mapping>
                <property>controls/switches/cws-bright</property>
            </binding>
        </hovered>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>cpswitch</object-name>
        <property>controls/switches/cws-bright</property>
        <factor>-45</factor>
        <offset-deg>20</offset-deg>
        <center>
          <x-m>-9.37678</x-m>
          <y-m>-0.256186</y-m>
          <z-m>-1.65109</z-m>
        </center>
        <axis>
          <x>0.0</x>
          <y>1.0</y>
          <z>0.0</z>
        </axis>
    </animation>

