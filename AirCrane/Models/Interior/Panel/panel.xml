    <object-name>panel1</object-name>
    <object-name>panel2</object-name>
    <object-name>panel3</object-name>
    <object-name>panel4</object-name>
  </effect>

  <!-- ai -->
  <model>
    <path>Aircraft/AirCrane/Models/Interior/Panel/Instruments/ai/ai.xml</path>
    <offsets>
      <x-m>-9.39 </x-m>
      <y-m> 0.453 </y-m>
      <z-m>-1.482 </z-m>
    </offsets>
  </model>

  <!-- asi -->
  <model>
    <path>Aircraft/AirCrane/Models/Interior/Panel/Instruments/asi/asi.xml</path>
    <offsets>
