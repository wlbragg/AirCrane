  <!-- non-Canvas remote Garmin 196 GPS (FGDATA) -->
  <!-- TODO Remove this old version, unhide and implement the Canvas version below -->
  <model>
    <name>Garmin196-GPS-non-Canvas-Passenger-View</name>
    <path>Aircraft/Instruments-3d/garmin196/garmin196.xml</path>
    <condition>
      <and>
        <property>sim/model/garmin196-visible</property>
        <or>
          <equals>
            <property>sim/current-view/name</property>
            <value>Passanger Seat View</value>
          </equals>
          <equals>
            <property>sim/current-view/name</property>
            <value>Crane Seat View</value>
          </equals>
        </or>
      </and>
    </condition>
    <offsets>
      <x-m>-9.36025</x-m>
      <y-m>-0.20100</y-m>
      <z-m>-1.21999</z-m>
      <heading-deg>0.0</heading-deg>
    </offsets>
  </model>

  <model>
    <name>Garmin196-GPS-non-Canvas-Pilot-View</name>
    <path>Aircraft/Instruments-3d/garmin196/garmin196.xml</path>
    <condition>
      <and>
        <property>sim/model/garmin196-visible</property>
        <equals>
          <property>sim/current-view/name</property>
          <value>Cockpit View</value>
        </equals>
      </and>
    </condition>
    <offsets>
      <x-m>-9.36025</x-m>
      <y-m>-0.20100</y-m>
      <z-m>-1.21999</z-m>
      <heading-deg>30.0</heading-deg>
    </offsets>
  </model>

