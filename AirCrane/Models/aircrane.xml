  <!-- Rotation de la roue avant en fonction de la gouverne de direction -->
  <animation>
    <name>EnsembleRoueAvant</name>
    <object-name>roueA</object-name>
    <object-name>axeAH</object-name>
    <object-name>axeAB</object-name>
    <object-name>articuleAH</object-name>
    <object-name>articuleAB</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>EnsembleRoueAvant</object-name>
    <property>gear/gear[0]/caster-angle-deg</property>
    <factor> -1 </factor>
    <center>
      <x-m> -8.740 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -2.989 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z>-1 </z>
    </axis>
  </animation>

  <!-- Compression -->
  <animation>
    <name>RoueAvant</name>
    <object-name>roueA</object-name>
    <object-name>axeAB</object-name>
    <object-name>articuleAB</object-name>
  </animation>

  <animation>
    <name>RoueGauche</name>
    <object-name>roueG</object-name>
    <object-name>roueGT</object-name>
    <object-name>axeGB</object-name>
    <object-name>articuleGB</object-name>
  </animation>

  <animation>
    <name>RoueDroite</name>
    <object-name>roueD</object-name>
    <object-name>roueT</object-name>
    <object-name>axeDB</object-name>
    <object-name>articuleDB</object-name>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>RoueAvant</object-name>
    <property>gear/gear[0]/compression-norm</property>
    <interpolation>
      <entry><ind> 0.000 </ind><dep> -0.10 </dep></entry>
      <entry><ind> 0.086 </ind><dep>  0.00 </dep></entry>
      <entry><ind> 1.000 </ind><dep>  0.11 </dep></entry>
    </interpolation>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>articuleAH</object-name>
    <property>gear/gear[0]/compression-norm</property>
